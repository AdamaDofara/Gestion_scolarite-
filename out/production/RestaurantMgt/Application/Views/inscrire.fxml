<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="503.0" prefWidth="854.0" style="-fx-background-color: #fff;"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Application.Controllers.LoginController">
    <children>
        <Pane layoutX="94.0" layoutY="46.0" prefHeight="432.0" prefWidth="687.0">
            <children>
                <Button fx:id="btnSignin" layoutX="234.0" layoutY="329.0" mnemonicParsing="false" prefHeight="34.0"
                        prefWidth="220.0" stylesheets="@../../resources/Styles/LoginStyling.css" text="Sign up"/>
                <Button fx:id="btnSignup" layoutX="599.0" layoutY="385.0" mnemonicParsing="false"
                        onAction="#GoBackSignIn_click" prefHeight="34.0" prefWidth="74.0"
                        stylesheets="@../../resources/Styles/LoginStyling.css" text="Sign in"/>
                <Separator layoutX="545.0" layoutY="363.0" orientation="VERTICAL" prefHeight="55.0" prefWidth="31.0"/>
                <Label alignment="CENTER" layoutX="514.0" layoutY="379.0" prefHeight="23.0" prefWidth="31.0"
                       style="-fx-background-color: #fff;" text="Or" textFill="#5b5a5a">
                    <font>
                        <Font name="Segoe UI" size="12.0"/>
                    </font>
                </Label>
                <Label fx:id="lblErrors" alignment="CENTER" layoutX="8.0" layoutY="95.0" prefHeight="23.0"
                       prefWidth="275.0" textFill="#ff6354">
                    <font>
                        <Font name="Segoe UI" size="10.0"/>
                    </font>
                </Label>
                <TextField fx:id="txtTelephone" layoutX="435.0" layoutY="89.0" prefHeight="34.0" prefWidth="220.0"
                           promptText="Telephone"/>
                <PasswordField fx:id="txtAdresse" layoutX="435.0" layoutY="159.0" prefHeight="34.0" prefWidth="220.0"
                               promptText="Adresse" stylesheets="@../../resources/Styles/LoginStyling.css"/>
                <PasswordField fx:id="txtPassword12" layoutX="435.0" layoutY="225.0" prefHeight="34.0" prefWidth="220.0"
                               promptText="password" stylesheets="@../../resources/Styles/LoginStyling.css"/>
                <TextField fx:id="txtMassar" layoutX="28.0" layoutY="24.0" prefHeight="34.0" prefWidth="220.0"
                           promptText="Code Massar"/>
                <PasswordField fx:id="txtNom" layoutX="28.0" layoutY="94.0" prefHeight="34.0" prefWidth="220.0"
                               promptText="Nom" stylesheets="@../../resources/Styles/LoginStyling.css"/>
                <PasswordField fx:id="txtPrenom" layoutX="28.0" layoutY="159.0" prefHeight="34.0" prefWidth="220.0"
                               promptText="Prenom" stylesheets="@../../resources/Styles/LoginStyling.css"/>
                <PasswordField fx:id="txtNaissance" layoutX="28.0" layoutY="225.0" prefHeight="34.0" prefWidth="220.0"
                               promptText="Date Naissance" stylesheets="@../../resources/Styles/LoginStyling.css"/>
                <PasswordField fx:id="txtEmail" layoutX="435.0" layoutY="24.0" prefHeight="34.0" prefWidth="220.0"
                               promptText="Email" stylesheets="@../../resources/Styles/LoginStyling.css"/>
            </children>
        </Pane>
        <ImageView fitHeight="41.0" fitWidth="34.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../../resources/images/LoginIcons/icons8_XING_100px.png"/>
            </image>
        </ImageView>
    </children>
</AnchorPane>
